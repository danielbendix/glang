fn solution() -> u64 {
    const size: usize = 20;

    const array = #allocate<u64>(size + 1);
    for i in 0 ... size {
        array[i] = 1;
    }
    // TODO: ignore binding
    for _ignore_ in 1 ... size {
        for i in 1 ... size {
            array[i] = array[i] + array[i - 1];
        }
    }

    return array[size];
}

fn main() {
    #print(solution());
}
