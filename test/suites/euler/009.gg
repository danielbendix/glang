fn solution() -> i64 {
    for a in 1 ..< 333 {
        const start = a + 1;
        const end = (1000 - a) / 2;
        for b in start ... end {
            const c = 1000 - b - a;
            if a * a + b * b == c * c {
                return a * b * c;
            }
        }
    }

    return -1;
}

fn main() {
    #print(solution());
}
