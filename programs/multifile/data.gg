struct List {
    var next: List*? = nil;
    var data: Data;
}

struct Data {
    const test: i64;
}

fn sum(list: List*?) -> i64 {
    var head = list;

    var total = 0;
    while unwrap current = head {
        total += current@.data.test;
        head = current@.next;
    }

    return total;
}
