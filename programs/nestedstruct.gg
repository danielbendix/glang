struct Parent {
    const age: i8;
    var child: Child;
}

struct Child {
    const age: i8;
    var parent: Parent*? = nil;
}

fn main() -> i32 {
    var parent: Parent = {
        age = 35,
        child = {
            age = 5,
        },
    };

    parent.child.parent = &parent;

    return 0;
}
