fn sum2(numbers: i64[!], n: i64) -> i64 {
    var sum: i64 = 0;

    var i: i64 = 0;
    while i < n {
        //sum += numbers[i];
        sum = sum + numbers[i];
        i = i + 1;
    }

    return sum;
}

fn add2(numbers: i64[]) -> i64 {
    let i0: i32 = 0;
    let i1: i32 = 1;
    return numbers[i0] * numbers[i1];
}

